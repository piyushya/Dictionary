lor: #818cab;\n  --font-color: #e1e1ff;\n  --bg-color: #161625;\n  --heading-color: #818cab;\n}\n\n/* */\n\n/* || GENERAL STYLES */\n\n/* *{\n  transition: all 0.2s;\n} */\n\nbody {\n  width: 100vw;\n  min-height: 100vh;\n  font-family: sans-serif;\n  background-color: var(--bg-color);\n  background-size: cover;\n  color: var(--font-color);\n  margin: 0;\n  padding: 0;\n  font-size: 18px;\n  overflow-x: hidden;\n}\n\nh1,\nh2,\nh3,\nh4 {\n  color: var(--heading-color);\n}\n\nli {\n  list-style: none;\n}\n\na {\n  text-decoration: none;\n  color: var(--secondary-color);\n}\n\np {\n  color: var(--primary-color);\n}\n\n/* */\n#top {\n  box-sizing: border-box;\n  width: 100vw;\n  min-height: 100vh;\n  display: flex;\n  flex-flow: row wrap;\n  background-color: var(--bg-color);\n}\n\n.left {\n  box-sizing: border-box;\n  flex: 1;\n  border-right: 10px solid var(--line-color);\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding-top: 2rem;\n}\n\n#header {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding: 1.5rem;\n}\n\n.heading {\n  font-size: 3rem;\n  margin: 0rem 2rem;\n}\n\n.right {\n  box-sizing: border-box;\n  flex: 2;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  padding: 2rem 3rem 1rem 3rem;\n  min-height: 100vh;\n}\n\n.search_form{\n  display: flex;\n  justify-content: center;\n  flex-flow: row wrap;\n}\n\n#nav {\n  width: 100%;\n}\n\n.recent_container{\n  padding: 0rem 1rem 0rem 1.5rem;\n  display: flex;\n  flex-flow: column-reverse;\n}\n\n.recent_container h2, .recent_container p{\n  font-size: 1rem;\n}\n\n.recent_search{\n  padding:0rem 1rem 0rem 1rem;\n}\n\n.recent_search:hover{\n  background-color: #8b9ed1;\n cursor: pointer;\n}\n\n.recent_search:hover .recent_definition{\n  height: auto;\n  overflow: auto;\n}\n\n.recent_definition{\n  overflow: hidden;\n  height: 1.1rem;\n}\n\n.nav_links {\n  padding: 0px;\n  width: 100%;\n  display: flex;\n  justify-content: center;\n}\n\n.nav_link {\n  padding: 0px 0.5rem 0px 0.5rem;\n}\n\n.link:hover {\n  color: var(--heading-color);\n}\n\n#search_text,\n.search_button {\n  box-sizing: border-box;\n  height: 3rem;\n  text-align: center;\n  font-size: 1.2rem;\n  margin-top: 1rem;\n}\n\n#search_text {\n  width: 16rem;\n}\n\n.search_button {\n  padding: 0px 20px 0px 20px;\n}\n\n#result {\n  margin-top: 1rem;\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.result_heading{\n  align-self: flex-start;\n}\n\n.part_result{\n  background-color: antiquewhite;\n  padding: 0.5rem 0.5rem 0.5rem 1rem;\n  margin-bottom: 1rem;\n  width: 80%;\n}\n\n.result_text {\n  margin-top: 0rem;\n  width: 100%;\n  max-width: 30rem;\n  height: auto;\n  display: flex;\n  flex-flow: column;\n  align-items: center;\n}\n\n.result_text h2{\n  font-size: 1rem;\n}\n\n.result_text h3{\n  font-size: 0.9rem;\n}\n\n.footer_text{\n  text-align: center;\n}\n"},"lineCount":214}},"error":null,"hash":"daf269d659402582e08de6c6bb63d4bc","cacheData":{"env":{}}}